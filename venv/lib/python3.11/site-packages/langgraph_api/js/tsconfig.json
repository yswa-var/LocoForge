{
  "compilerOptions": {
    "module": "NodeNext",
    "jsx": "react-jsx",
    "noEmit": true,
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowImportingTsExtensions": true,
    "forceConsistentCasingInFileNames": true
  }
}
